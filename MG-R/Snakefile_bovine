#load  samples  into  table
import  pandas  as pd
import os

# TaxSBP failed for 64 bins
# If you specify 128 bins for TaxSBP it creates *approximately* 128 bins (so 153 bins were created)
bovine_bins = list(range(0, 153))

rule make_all:
	input:
		expand("bovine_out/{bin}.fasta", bin = bovine_bins)
	shell:
		"echo 'Done'"

include: "rules/bovine.smk"
